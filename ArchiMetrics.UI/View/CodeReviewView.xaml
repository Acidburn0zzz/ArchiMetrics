<UserControl x:Class="ArchiMetrics.UI.View.CodeReviewView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:viewModel="clr-namespace:ArchiMetrics.UI.ViewModel"
			 xmlns:mui="http://firstfloorsoftware.com/ModernUI"
			 xmlns:view="clr-namespace:ArchiMetrics.UI.View"
			 mc:Ignorable="d"
			 d:DesignHeight="300"
			 d:DesignWidth="300"
			 d:DataContext="{d:DesignInstance Type=viewModel:CodeReviewViewModel}">
	<mui:ModernTab SelectedSource="View/Tabs/CodeReviewDataGridView.xaml"
				   ContentLoader="{DynamicResource Loader}">
		<mui:ModernTab.Links>
			<mui:Link DisplayName="List"
					  Source="View/Tabs/CodeReviewDataGridView.xaml" />
			<mui:Link DisplayName="Kind"
					  Source="View/Tabs/CodeErrorGraphView.xaml" />
		</mui:ModernTab.Links>
	</mui:ModernTab>
</UserControl>
