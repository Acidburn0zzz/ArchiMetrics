<UserControl x:Class="ArchiMetrics.UI.View.ValidateStructureView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             xmlns:localization="clr-namespace:ArchiMetrics.Localization;assembly=ArchiMetrics.Localization"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<mui:ModernTab SelectedSource="View/Tabs/DefineStructureRulesView.xaml"
				   ContentLoader="{DynamicResource Loader}">
		<mui:ModernTab.Links>
			<mui:Link DisplayName="{x:Static localization:Strings.Define}"
					  Source="View/Tabs/DefineStructureRulesView.xaml" />
			<mui:Link DisplayName="{x:Static localization:Strings.Transform}"
					  Source="View/Tabs/ModelTransformationView.xaml" />
			<mui:Link DisplayName="{x:Static localization:Strings.Results}"
					  Source="View/Tabs/ModelValidationResultView.xaml" />
		</mui:ModernTab.Links>
	</mui:ModernTab>
</UserControl>
